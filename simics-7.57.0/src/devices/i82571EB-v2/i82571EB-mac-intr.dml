dml 1.4;

param ITR_UNIT = 2.56e-7; /* 256 ns */

param INTR_TXDW   = 1 << 0;  // Transmit Descriptor Written Back
param INTR_TXQE   = 1 << 1;  // Transmit Queue Empty
param INTR_LSC    = 1 << 2;  // Link Status Change
param INTR_RXDMT0 = 1 << 4;  // Receive Descriptor Minimum Threshold Hit
param INTR_RXO    = 1 << 6;  // Receive Overrun
param INTR_RXT0   = 1 << 7;  // Receive Timer Interrupt
param INTR_MDAC   = 1 << 9;  // MDI/O Access Complete
param INTR_TXD_LOW = 1 << 15; // Transmit Descriptor Low Threshold Hit
param INTR_SRPD   = 1 << 16; // Small Receive Packet Detected
param INTR_ACK    = 1 << 17; // Receive ACK Frame Detected
